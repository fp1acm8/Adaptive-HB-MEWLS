{
  "dataset": {
    "path": "data/example13_dataset.txt",
    "normalize": true
  },
  "noise": {
    "type": "gaussian",
    "standardDeviation": 0.05,
    "outlierFraction": 0.05,
    "outlierInflation": 0.2,
    "seed": 42
  },
  "solvers": [
    {
      "name": "LeastSquares",
      "function": "adaptivehb.solvers.leastSquaresSolver",
      "parameters": {
        "degree": 3
      }
    },
    {
      "name": "MEWLS",
      "function": "adaptivehb.solvers.mewlsSolver",
      "parameters": {
        "degree": 3,
        "lambda": 15.0
      }
    }
  ],
  "metrics": [
    "rmse",
    "maxAbsError",
    "mae"
  ]
}
